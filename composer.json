{
  "name": "arquivei/lite-api-php",
  "description": "Library that implements the Arquivei Lite Api.",
  "license": "Apache-2.0",
  "keywords": [
    "arquivei",
    "lite",
    "sdk",
    "api",
    "lite-api"
  ],
  "require": {
    "php": "^7.1.3",
    "monolog/monolog": "^1.23",
    "guzzlehttp/guzzle": "^6.5",
    "vlucas/phpdotenv": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "files": [
      "bootstrap.php"
    ],
    "psr-4": {
      "Arquivei\\LiteApi\\Sdk\\": "src/",
      "Arquivei\\LiteApi\\Sdk\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
